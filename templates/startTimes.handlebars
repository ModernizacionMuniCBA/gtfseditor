<button class="btn btn-default add-btn">Add</button>
<button class="btn btn-default save-btn">Save</button>
<table class="table table-hover table-condensed">
<thead>
	<tr>
		<th>Stop Id</th>
		<th>Time</th>
	</tr>
</thead>
<tbody>
	{{#each this}}
	<tr data-stop-id="{{stop_id}}" draggable="true">
		<td>{{service_id}}</td>
		<td class="stop-time" contenteditable="true">{{start_time}}</td>
	</tr>
	{{/each}}
</tbody>
</table>