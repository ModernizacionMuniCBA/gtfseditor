<table class="table table-hover table-condensed">
<thead>
	<tr>
		<th>Stop Id</th>
		<th>Time</th>
		<th>Distance (km)</th>
	</tr>
</thead>
<tbody>
	{{#each this}}
	<tr data-stop-id="{{stop_seq.stop_id}}" draggable="true">
		<td>{{stop_seq.stop_id}}</td>
		<td class="stop-time" contenteditable="true">{{stop_seq.stop_time}}</td>
		<td>{{stop_seq.shape_dist_traveled}}</td>
	</tr>
	{{/each}}
</tbody>
</table>