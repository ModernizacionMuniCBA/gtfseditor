<table class="table table-hover table-condensed">
<thead>
	<tr>
		<th>Stop Id</th>
		<th>Time</th>
		<th>Distance (km)</th>
	</tr>
</thead>
<tbody>
	{{#each this}}
	<tr>
		<td>{{id}}</td>
		<td>{{properties.stop_time}}</td>
		<td>{{properties.shape_dist_traveled}}</td>
	</tr>
	{{/each}}
</tbody>
</table>